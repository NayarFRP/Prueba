<odoo>
  <data>
    <record id="stock_warehouse_inherit_sale_ventacero" model="ir.ui.view">
      <field name="name">stock.warehouse.inherit.sale.ventacero</field>
      <field name="model">stock.warehouse</field>
      <field name="inherit_id" ref="stock.view_warehouse"/>
      <field name="arch" type="xml">
        
        <xpath expr="//field[@name='delivery_steps']" position="after">
          <field name="entrega_turnos" widget="boolean_toggle"/>
        </xpath>

      </field>
    </record>
  </data>
</odoo>